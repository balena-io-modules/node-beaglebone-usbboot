{
	"compilerOptions": {
		"typeRoots": ["typings", "node_modules/@types"],
		"module": "commonjs",
		"outDir": "./lib",
		"sourceMap": true,
		"declaration": true,
		"target": "es6",
		"strict": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strictNullChecks": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	]
}